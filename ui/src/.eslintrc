{
  "extends": "airbnb",
  "env": {
    "browser": true
  },
  "rules": {
    "import/extensions": [ "error", "always", { "ignorePackages": true } ],
    "react/prop-types": "off",
    "react/jsx-one-expression-per-line": "off",
    "linebreak-style": "off",
    "no-underscore-dangle": "off",
    "no-nested-ternary": "off",
    "class-methods-use-this": "off"
  }
}
